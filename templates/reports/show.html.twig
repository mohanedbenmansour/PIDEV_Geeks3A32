{% extends 'base.html.twig' %}

{% block title %}Reports{% endblock %}

{% block body %}
    <h1>Reports</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ report.id }}</td>
            </tr>
            <tr>
                <th>Object_id</th>
                <td>{{ report.objectId }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{{ report.etat ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ report.content }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('reports_index') }}">back to list</a>

    <a href="{{ path('reports_edit', {'id': report.id}) }}">edit</a>

    {{ include('reports/_delete_form.html.twig') }}
{% endblock %}
